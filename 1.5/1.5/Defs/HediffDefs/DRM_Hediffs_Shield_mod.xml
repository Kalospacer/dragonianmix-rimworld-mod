        </descriptionHyperlinks>
        <costList>
            <Plasteel>15</Plasteel>
            <Uranium>40</Uranium>
            <Gold>1</Gold>
            <ComponentSpacer>2</ComponentSpacer>
        </costList>
        <recipeMaker>
            <researchPrerequisite>ShieldBelt</researchPrerequisite>
            <recipeUsers>                <!--工作于-->
